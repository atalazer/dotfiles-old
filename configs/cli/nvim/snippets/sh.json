{
  "check": {
    "prefix": "check",
    "body": "command -v \"${1:which_cmd}\" > /dev/null 2>&1",
    "description": "Check if command is exist"
  },
  "silent": {
    "prefix": "Silent",
    "body": ">/dev/null 2>&1",
    "description": "silent command output"
  },
  "modeline": {
    "prefix": ["vim", "modeline"],
    "body": "# vim: ft=sh ts=4 sw=4:",
    "description": "Add vim modeline"
  },
  "shebang": {
    "prefix": ["#!", "shebang"],
    "body": "#!/usr/bin/env bash",
    "description": "Add shebang for scripting"
  },
  "help": {
    "prefix": "help",
    "body": [
      "help() {",
      "cat <<EOF",
      "Usage for \\$NAME:",
      "  Command:",
      "  Options",
      "    -h, --help      Show this message",
      "    -v, --version   Show version",
      "EOF",
      "}"
    ],
    "description": "Make help"
  },
  "version": {
    "prefix": "version",
    "body": [
      "version() {",
      "cat <<EOF",
      "\\$NAME \\$VERSION",
      "EOF",
      "}"
    ],
    "description": "make version"
  },
  "init": {
    "prefix": "init",
    "body": [
      "NAME=$1",
      "AUTHOR:$2",
      "VERSION=v0.1"
    ],
    "description": "Init Script"
  },
  "main": {
    "prefix": "main",
    "body": [
      "while [ -n \"\\$1\" ]; do",
      "\tcase \"\\$1\" in",
      "\t\t\"-v\" | \"--version\") version ;;",
      "\t\t\"-h\" | \"--help\") help ;;",
      "\t\t*) echo \"Invalid\" && help ;;",
      "\tesac",
      "\tshift",
      "done"
    ],
    "description": "Main Program"
  }
}
